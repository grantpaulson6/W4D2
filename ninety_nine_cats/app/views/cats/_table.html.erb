

<a href="<%= cats_url %>">All Cats</a>
<br>

<table>
  <thead>
    <tr>
      <th>Attribute</th>
      <th>Value</th>
    </tr>
  </thead>

  <tbody>
  
  <% cat.attributes.each_pair do |name, value| %>
    <tr>
      <td><%= name %></td>
      <td><%= value %></td>
    </tr>
  <% end %>
  </tbody>

</table>
